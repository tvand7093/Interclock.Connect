<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms"
	xmlns:ctrls="clr-namespace:InterClock.Connect.Data.Controls;assembly=InterClock.Connect.Data"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	x:Class="InterClock.Connect.Data.Controls.CircularButtonsView">
	<ContentView.Content>
		<StackLayout Orientation="Horizontal" Padding="10">
			<ctrls:CircularButton x:Name="LeftButton" HorizontalOptions="CenterAndExpand"/>
			<ctrls:CircularButton x:Name="CenterButton" HorizontalOptions="CenterAndExpand" />
			<ctrls:CircularButton x:Name="RightButton" HorizontalOptions="CenterAndExpand"/>
		</StackLayout>
	</ContentView.Content>
</ContentView>
